            <widget type="space" />
            <widget type="pushbutton">
                <property name="label">隐藏</property>
                <property name="bitmap">hide</property>
                <property name="width">70</property>
                <property name="onClick">#evtHide</property>
            </widget>
            <widget type="pushbutton">
                <property name="label">终止服务</property>
                <property name="bitmap">outdoor</property>
                <property name="width">70</property>
                <property name="onClick">#evtExit</property>
            </widget>
        </widget>
    </widget>
